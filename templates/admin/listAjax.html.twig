
                          <h4 class="mt-0 header-title">Users Table</h4>
                                    
                                        <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap">
                                            <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Username</th>
                                                <th>Phone</th>
                                                <th>Email</th>
                                                <th>Name</th>
                                                <th>LastName</th>
                                                <th>Roles</th>
                                                   <th></th>
                                            </tr>
                                            </thead>
    
    
                                            <tbody>
                                     {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.username }}</td>
                <td>{{ user.phone }}</td>
                <td>{{ user.email }}</td>
                <td>{{ user.name }}</td>
                <td>{{ user.lastName }}</td>
                <td> {% for role in user.roles %}
                {{ role }}
              {% endfor %}</td>
                <td>
                  {% include '/user/_delete_form.html.twig' %}
                   
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
                                            
                                            </tbody>
                                        </table>